<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ljq.bishe.mapper.LoginMapper">
    <select id="getstu" resultType="Student">
        SELECT * FROM student WHERE stuname = #{stuname}
    </select>
    <select id="gettea" resultType="Teacher">
        SELECT * FROM teacher WHERE teaname = #{teaname}
    </select>

</mapper>